{
  "name": "foxes-plugin",
  "version": "1.1.3",
  "description": "基于 mirai-foxes 的插件管理器",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "lint": "eslint ./src",
    "lint:type": "tsc --noEmit -p ./tsconfig.json",
    "fix": "eslint ./src --fix",
    "build": "tsup"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FurDevsCN/foxes-plugin.git"
  },
  "types": "./dist/index.d.ts",
  "author": "FurDevsCN",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/FurDevsCN/foxes-plugin/issues"
  },
  "homepage": "https://github.com/FurDevsCN/foxes-plugin#readme",
  "dependencies": {
    "mirai-foxes": "^1.5.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "typescript": "^5.1.6",
    "tsup": "^6.5.0"
  }
}
